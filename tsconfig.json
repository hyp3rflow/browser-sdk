{
  "compilerOptions": {
    "target": "es2019",
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "strict": true,
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  }
}
